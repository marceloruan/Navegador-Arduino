 #include <SPI.h>
 #include <nRF24L01.h>
 #include <RF24.h>
 
  RF24 radio(7, 8);
  unsigned long startTime;
  const byte rxAddr[6] = "00001";
  int count;
  void setup()
  {
   while (!Serial);
   Serial.begin(9600);
   
   radio.begin();
   radio.openReadingPipe(0, rxAddr);
   
   radio.startListening();
 }
  
  void loop()
  {
    if (radio.available())
    {
      char text[32] = {0};

    startTime=millis();
    radio.read(&text, sizeof(text));
    count++;
        Serial.println(text);

        if(count==20){
        
          float media=(startTime/count);
          Serial.println(media);
           count=0;
          media=millis()-  startTime;
          
          }
     
       
 
    }
  }
